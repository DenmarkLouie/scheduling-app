<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_3157"
    tools:context=".UserDashboard">

    <EditText
        android:id="@+id/FullName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/gotoMain"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="289dp"
        android:layout_marginEnd="-2dp"
        android:drawableStart="@drawable/ic_person"
        android:hint="Full Name" />

    <EditText
        android:id="@+id/SchoolName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/FullName"
        android:drawableStart="@drawable/ic_school"
        android:hint="School Name" />

    <EditText
        android:id="@+id/ContactNum"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/SchoolName"
        android:drawableStart="@drawable/ic_number"
        android:hint="Contact Number"
        android:inputType="phone"/>

    <EditText
        android:id="@+id/ContactAdd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ContactNum"
        android:drawableStart="@drawable/ic_address"
        android:hint="Address" />

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="268dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="4dp"
        android:text="Logout" />

    <Button
        android:id="@+id/gotoMain"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/logout"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="8dp"
        android:text="Home" />

    <Button
        android:id="@+id/UpdateButton"
        android:layout_width="124dp"
        android:layout_height="58dp"
        android:layout_below="@id/ContactAdd"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="143dp"
        android:layout_marginTop="10dp"
        android:text="UPDATE" />

    <Button
        android:id="@+id/CancelUpdate"
        android:layout_width="wrap_content"
        android:layout_height="58dp"
        android:layout_below="@id/SaveUpdate"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:text="Cancel" />

    <TextView
        android:id="@+id/accesslevel"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginLeft="150dp"
        android:layout_marginRight="150dp"
        android:text="Staff"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:layout_marginTop="15dp"/>

    <Button
        android:id="@+id/SaveUpdate"
        android:layout_width="wrap_content"
        android:layout_height="58dp"

        android:layout_below="@id/UpdateButton"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"

        android:text="SAVE" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="275dp"
        android:layout_height="215dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="64dp"
        app:srcCompat="@drawable/creocalndr_logo" />

    <TextView
        android:id="@+id/changePassword"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView3"
        android:layout_marginTop="10dp"
        android:clickable="true"
        android:text="Change Password"
        android:textAlignment="center"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="15sp"/>

</RelativeLayout>